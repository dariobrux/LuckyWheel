<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyTextView">
        <attr name="font" format="reference" />
    </declare-styleable>

    <declare-styleable name="LuckyWheelView">
        <attr name="lkw_backgroundColor" format="color" />
        <attr name="lkw_cursor" format="reference" />
        <attr name="lkw_centerImage" format="reference" />
        <attr name="lkw_titleColor" format="color" />
        <attr name="lkw_titleSize" format="float" />
        <attr name="lkw_subtitleSize" format="float" />
        <attr name="lkw_subtitleColor" format="color" />
        <attr name="lkw_strokeColor" format="color" />
        <attr name="lkw_firstTitleColor" format="color" />
        <attr name="lkw_firstSubtitleColor" format="color" />
        <attr name="lkw_letterSpacing" format="float" />
        <attr name="lkw_animationDuration" format="integer" />
    </declare-styleable>

</resources>